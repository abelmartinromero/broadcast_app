<h3>Red Alerts ({{redAlert.length}}) </h3>
<br/>
<ol>
{{#each redAlert}}
<li><a id="{{id}}" class="link" href="{{setting 'domain'}}/agent/tickets/{{id}}">{{subject}}</a></li>
{{/each}}
</ol>
<h3>Problems ({{problems.length}})</h3>
<ol>
{{#each problems}}
<li><a id="{{id}}" class="link" href="{{setting 'domain'}}/agent/tickets/{{id}}">{{subject}}</a></li>
{{/each}}
</ol>